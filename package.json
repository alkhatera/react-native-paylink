{
  "name": "react-native-paylink",
  "version": "1.0.3",
  "description": "React native package to process payments using Paylink as a payment gateway for iOS, Android and Web apps",
  "source": "./src/index.tsx",
  "main": "./lib/commonjs/index.js",
  "module": "./lib/module/index.js",
  "scripts": {
    "example": "yarn workspace react-native-paylink-example",
    "test": "jest",
    "typecheck": "tsc",
    "build:force": "tsc --skipLibCheck --noEmitOnError false",
    "lint": "eslint \"**/*.{js,ts,tsx}\"",
    "clean": "del-cli lib",
    "prepare": "bob build",
    "release": "release-it"
  },
  "exports": {
    ".": {
      "import": {
        "types": "./lib/typescript/module/src/index.d.ts",
        "default": "./lib/module/index.js"
      },
      "require": {
        "types": "./lib/typescript/commonjs/src/index.d.ts",
        "default": "./lib/commonjs/index.js"
      }
    }
  },
  "files": [
    "src",
    "lib",
    "android",
    "ios",
    "cpp",
    "*.podspec",
    "!ios/build",
    "!android/build",
    "!android/gradle",
    "!android/gradlew",
    "!android/gradlew.bat",
    "!android/local.properties",
    "!**/__tests__",
    "!**/__fixtures__",
    "!**/__mocks__",
    "!**/.*"
  ],
  "keywords": [
    "react-native",
    "ios",
    "android"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alkhatera/react-native-paylink.git"
  },
  "author": "alkhatera <alkhateralaa@outlook.com> (https://github.com/alkhatera)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/alkhatera/react-native-paylink/issues"
  },
  "homepage": "https://github.com/alkhatera/react-native-paylink#readme",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "dependencies": {
    "@devvie/bottom-sheet": "^0.4.3",
    "@react-native-clipboard/clipboard": "^1.16.2",
    "libphonenumber-js": "^1.12.4",
    "lucide-react-native": "^0.477.0",
    "moment": "^2.30.1",
    "react-native-credit-card-input": "^1.0.0",
    "react-native-international-phone-number": "^0.9.1",
    "react-native-otp-entry": "^1.8.3",
    "react-native-really-awesome-button": "^2.0.4"
  },
  "peerDependencies": {
    "expo-font": ">=13.0.0",
    "react": "*",
    "react-native": "*",
    "react-native-safe-area-context": ">=4.0.0",
    "react-native-svg": ">=13.0.0",
    "react-native-webview": ">=11.0.0"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^19.7.1",
    "@evilmartians/lefthook": "^1.11.2",
    "@react-native/eslint-config": "^0.73.1",
    "@release-it/conventional-changelog": "^10.0.0",
    "@types/jest": "^29.5.14",
    "@types/react": "^19.0.10",
    "commitlint": "^19.7.1",
    "del-cli": "^6.0.0",
    "eslint": "^8.51.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "jest": "^29.7.0",
    "prettier": "^3.0.3",
    "react": "18.3.1",
    "react-native": "0.76.7",
    "react-native-builder-bob": "^0.37.0",
    "react-native-safe-area-context": "^5.3.0",
    "react-native-svg": "^15.11.2",
    "react-native-webview": "^13.13.2",
    "release-it": "^18.1.2",
    "typescript": "^5.8.2"
  },
  "resolutions": {
    "@types/react": "^18.2.44"
  },
  "workspaces": [
    "example"
  ],
  "packageManager": "yarn@3.6.1",
  "jest": {
    "preset": "react-native",
    "modulePathIgnorePatterns": [
      "<rootDir>/example/node_modules",
      "<rootDir>/lib/"
    ]
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "release-it": {
    "git": {
      "commitMessage": "chore: release ${version}",
      "tagName": "v${version}"
    },
    "npm": {
      "publish": true
    },
    "github": {
      "release": true
    },
    "plugins": {
      "@release-it/conventional-changelog": {
        "preset": "angular"
      }
    }
  },
  "eslintConfig": {
    "root": true,
    "extends": [
      "@react-native",
      "prettier"
    ],
    "rules": {
      "react/react-in-jsx-scope": "off",
      "prettier/prettier": [
        "error",
        {
          "quoteProps": "consistent",
          "singleQuote": true,
          "tabWidth": 2,
          "trailingComma": "es5",
          "useTabs": false
        }
      ]
    }
  },
  "eslintIgnore": [
    "node_modules/",
    "lib/"
  ],
  "prettier": {
    "quoteProps": "consistent",
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "es5",
    "useTabs": false
  },
  "react-native-builder-bob": {
    "source": "src",
    "output": "lib",
    "targets": [
      [
        "commonjs",
        {
          "esm": true
        }
      ],
      [
        "module",
        {
          "esm": true
        }
      ],
      [
        "typescript",
        {
          "esm": true
        }
      ]
    ]
  },
  "create-react-native-library": {
    "type": "library",
    "version": "0.41.2"
  }
}
